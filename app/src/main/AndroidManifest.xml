<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RoomFinder"
        tools:targetApi="31">
        <activity
            android:name=".view.Pending"
            android:exported="false"
            android:label="@string/title_activity_pending"
            android:theme="@style/Theme.RoomFinder" />
        <activity
            android:name=".view.RequestForm"
            android:exported="false"
            android:label="@string/title_activity_request_form"
            android:theme="@style/Theme.RoomFinder" />
        <activity
            android:name=".view.Details"
            android:exported="false"
            android:label="@string/title_activity_detail"
            android:theme="@style/Theme.RoomFinder" />
        <activity
            android:name=".view.Settings"
            android:exported="false"
            android:label="@string/title_activity_settings"
            android:theme="@style/Theme.RoomFinder" />
        <activity
            android:name=".view.SignIn"
            android:exported="true"
            android:label="@string/title_activity_signup"
            android:theme="@style/Theme.RoomFinder" /> <!-- <intent-filter> -->
        <!-- <action android:name="android.intent.action.MAIN" /> -->
        <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
        <!-- </intent-filter> -->
        <!-- </activity> -->
        <activity
            android:name=".view.SignUp"
            android:exported="false"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.RoomFinder" />
        <activity
            android:name=".view.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.RoomFinder">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>